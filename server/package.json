    "express": "^4.16.2",
    "express-ws": "^4.0.0",
    "firebase-admin": "^8.6.0",
    "geoip-lite": "^1.3.8",
    "invariant": "^2.2.2",
    "lib": "0.0.1",
    "lodash": "^4.17.5",
    "multer": "^1.4.1",
    "mysql2": "^1.5.1",
    "node-schedule": "^1.3.0",
    "nodemailer": "^4.4.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "^3.1.3",
    "react-html-email": "^3.0.0",
