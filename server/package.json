    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "chokidar-cli": "^2.0.0",
    "concurrently": "^3.5.1",
    "flow-bin": "^0.98.0",
